#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
int main()
{
    int a ,n;
    printf("Enter a number: ");
    scanf(" %d", &a);
    n=a;
    int count=0;
    while(n!=0){
        n=n/10;
        count++;
    }
    printf("Number of Digits: %d", count);
    return 0;
}
